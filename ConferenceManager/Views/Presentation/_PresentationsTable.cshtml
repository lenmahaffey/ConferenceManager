@model IEnumerable<Presentation>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Conference</th>
            <th>Name</th>
            <th>Description</th>
            <th>Presenter</th>
            <th>Start</th>
            <th>End</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (Presentation p in Model)
        {
            <tr>
                <td>@p.Name</td>
                <td>@p.Description</td>
                <td>@p.Presenter.FirstName @p.Presenter.LastName</td>
                <td>@p.StartTime.ToShortDateString() <br /> @p.StartTime.ToShortTimeString()</td>
                <td>@p.EndTime.ToShortDateString() <br /> @p.EndTime.ToShortTimeString()</td>
                <td>
                    <a asp-action="ViewPresentation"
                       asp-route-id="@p.ID"
                       class="btn btn-secondary">View</a>
                </td>
                <td>
                    <a asp-action="EditPresentation"
                       asp-route-id="@p.ID"
                       class="btn btn-danger">Edit</a>
                </td>
            </tr>
        }
    </tbody>
</table>